# platform :ios, '9.0'

def framework_pods

pod 'Alamofire'
use_frameworks!

end


target 'FileList' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  framework_pods
  use_frameworks!

  # Pods for FileList

  target 'FileListTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'FileListUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end

